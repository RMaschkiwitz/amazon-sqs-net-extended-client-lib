{
    "version": "2.0.0",
    "command": "dotnet",
    "args": [],
    "options": {
        "cwd": "${workspaceRoot}/"
    },
    "tasks": [
        {
            "label": "build",
            "group": "build",
            "args": [
                "${workspaceRoot}/src/Amazon.SQS.ExtendedClient"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "test",
            "group": "test",
            "presentation": {
                "reveal": "always"
            },
            "args": [
                "${workspaceRoot}/test/Amazon.SQS.ExtendedClient.Tests"
            ],
        }
    ]
}